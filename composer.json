{
  "name": "spryker/architecture-sniffer",
  "type": "library",
  "description": "architecture-sniffer module",
  "homepage": "http://spryker.com",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "phpmd/phpmd": "^2.4",
    "symfony/finder": "*",
    "zendframework/zend-config": "*",
    "zendframework/zend-filter": "*",
    "roave/better-reflection": "*"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "phpstan/phpstan": "^0.9.2",
    "phpunit/phpunit": "~5.2",
    "spryker/code-sniffer": "*",
    "symfony/var-dumper": "^4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "keywords": [
    "framework",
    "sniffer",
    "architecture",
    "standards"
  ],
  "authors": [
    {
      "name": "Spryker",
      "homepage": "http://spryker.com"
    }
  ],
  "support": {
    "source": "https://github.com/spryker/architecture-sniffer"
  },
  "autoload": {
    "psr-4": {
      "ArchitectureSniffer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArchitectureSnifferTest\\": "tests/"
    }
  },
  "include-path": [
    "src/"
  ]
}
